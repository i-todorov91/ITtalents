import java.util.Collections;
import java.util.Map.Entry;
import java.util.Scanner;
import java.util.TreeMap;

public class MostCommonLetters {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		System.out.println("Please enter some text:");
		String text = sc.nextLine();
		String temp = text.toLowerCase();
		int numOfLetters = 0;
		
		TreeMap<Character , Integer> charTree = new TreeMap();
		
		for (int i = 0; i < temp.length(); i++) {
			char symbol = text.charAt(i);
			if (symbol >= 'a' && symbol <= 'z') {
				if (!charTree.containsKey(symbol)) {
					charTree.put(symbol, 0);
				}
				int counter = charTree.get(symbol);
				charTree.put(symbol, ++counter);
				numOfLetters++;
			}
		}
		
		TreeMap<Integer, String> charTreeSorded = new TreeMap<>();
		for (Entry<Character, Integer> entry : charTree.entrySet()) {
			if (!charTreeSorded.containsKey(entry.getValue())) {
				charTreeSorded.put(entry.getValue(), "");
			}
			String letters = charTreeSorded.get(entry.getValue());
			charTreeSorded.put(entry.getValue(), letters + entry.getKey());
		}
		TreeMap<Integer, String> charTreeSordedDescending = new TreeMap<Integer, String>(Collections.reverseOrder());
		System.out.println(charTreeSorded);
		System.out.println(charTreeSordedDescending);
		
	}
}
