
public class Demo {

	public static void main(String[] args) throws Exception {
		
		ElectronicSecuredNotepad iPod = new ElectronicSecuredNotepad(30, "qweR1");
		iPod.start();
		iPod.getPages()[0].setTitle("Zaglavie");
		iPod.addText("Malko textche", 1);
		iPod.printPages();
		iPod.overwritePage("poveche text", 1);
		iPod.printPages();
		
	}
}
